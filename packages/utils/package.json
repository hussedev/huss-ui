    "lint": "eslint \"src/**/*.ts*\"",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@hussexample/tsconfig": "workspace:*",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.57.0",
    "@hussexample/eslint-config": "workspace:*",
    "tsup": "^8.0.2",
    "typescript": "5.5.4"
  },
  "dependencies": {
    "react": "^18.2.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
